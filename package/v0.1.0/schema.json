{
  "$schema": "https://json-schema.org/draft-07/schema",
  "additionalProperties": false,
  "definitions": {
    "Features": {
      "additionalProperties": false,
      "properties": {
        "vert": {
          "default": false,
          "title": "Vert",
          "type": "boolean"
        }
      },
      "title": "Features",
      "type": "object"
    },
    "Font": {
      "additionalProperties": false,
      "properties": {
        "filename": {
          "title": "Filename",
          "type": "string"
        },
        "number": {
          "default": 0,
          "title": "Number",
          "type": "integer"
        },
        "sha256": {
          "title": "Sha256",
          "type": "string"
        }
      },
      "required": ["filename", "sha256"],
      "title": "Font",
      "type": "object"
    },
    "FontWeight": {
      "additionalProperties": false,
      "properties": {
        "black": {
          "$ref": "#/definitions/Font"
        },
        "bold": {
          "$ref": "#/definitions/Font"
        },
        "extraBold": {
          "$ref": "#/definitions/Font"
        },
        "extraLight": {
          "$ref": "#/definitions/Font"
        },
        "light": {
          "$ref": "#/definitions/Font"
        },
        "medium": {
          "$ref": "#/definitions/Font"
        },
        "normal": {
          "$ref": "#/definitions/Font"
        },
        "semiBold": {
          "$ref": "#/definitions/Font"
        },
        "thin": {
          "$ref": "#/definitions/Font"
        }
      },
      "title": "FontWeight",
      "type": "object"
    },
    "Source": {
      "additionalProperties": false,
      "properties": {
        "fonts": {
          "$ref": "#/definitions/FontWeight"
        },
        "url": {
          "format": "uri",
          "maxLength": 65536,
          "minLength": 1,
          "title": "Url",
          "type": "string"
        }
      },
      "required": ["url", "fonts"],
      "title": "Source",
      "type": "object"
    }
  },
  "description": "OpenFonts.jp font package schema generated by pydantic.",
  "properties": {
    "$schema": {
      "format": "uri",
      "maxLength": 65536,
      "minLength": 1,
      "title": "$Schema",
      "type": "string"
    },
    "authors": {
      "items": {
        "type": "string"
      },
      "title": "Authors",
      "type": "array"
    },
    "category": {
      "enum": ["Mincho", "Gothic", "MaruGothic", "Brash", "HandWriting", "Artistic", "Others"],
      "title": "Category",
      "type": "string"
    },
    "characters": {
      "items": {
        "enum": ["Alphabet", "Hiragana", "Katakana", "Kanji"],
        "type": "string"
      },
      "title": "Characters",
      "type": "array"
    },
    "copyrights": {
      "items": {
        "type": "string"
      },
      "title": "Copyrights",
      "type": "array"
    },
    "description": {
      "title": "Description",
      "type": "string"
    },
    "features": {
      "$ref": "#/definitions/Features"
    },
    "homepage": {
      "format": "uri",
      "maxLength": 65536,
      "minLength": 1,
      "title": "Homepage",
      "type": "string"
    },
    "id": {
      "pattern": "^[a-z][a-z0-9-]*$",
      "title": "Id",
      "type": "string"
    },
    "license": {
      "enum": ["MIT", "IPA", "OFL-1.1", "Apache-2.0", "BSD-3-Clause", "Mplus"],
      "title": "License",
      "type": "string"
    },
    "name": {
      "title": "Name",
      "type": "string"
    },
    "sources": {
      "items": {
        "$ref": "#/definitions/Source"
      },
      "title": "Sources",
      "type": "array"
    },
    "version": {
      "title": "Version",
      "type": "string"
    }
  },
  "required": [
    "$schema",
    "id",
    "name",
    "version",
    "homepage",
    "license",
    "authors",
    "sources",
    "features",
    "category",
    "characters",
    "copyrights"
  ],
  "title": "Package",
  "type": "object"
}
